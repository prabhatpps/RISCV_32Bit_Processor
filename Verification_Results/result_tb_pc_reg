    %jmp T_4.1;
T_4.0 ;
    %vpi_call 2 222 "$display", "STATUS: SOME TESTS FAILED" {0 0 0};
T_4.1 ;
    %vpi_call 2 225 "$display", "====================================================" {0 0 0};
    %vpi_call 2 226 "$display", "\000" {0 0 0};
    %vpi_call 2 228 "$finish" {0 0 0};
    %end;
    .thread T_4;
