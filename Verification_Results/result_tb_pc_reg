TD_tb_pc_reg.check ;
    %load/vec4 v0x58c9a243eba0_0;
    %load/vec4 v0x58c9a2466c70_0;
    %cmp/e;
    %jmp/0xz  T_0.0, 6;
    %load/vec4 v0x58c9a24aebd0_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58c9a24aebd0_0, 0, 32;
    %vpi_call 2 78 "$display", "[PASS] %s | Expected = 0x%08h, Got = 0x%08h", v0x58c9a24748c0_0, v0x58c9a2466c70_0, v0x58c9a243eba0_0 {0 0 0};
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x58c9a24aeb10_0;
    %addi 1, 0, 32;
    %store/vec4 v0x58c9a24aeb10_0, 0, 32;
    %vpi_call 2 83 "$display", "[FAIL] %s | Expected = 0x%08h, Got = 0x%08h", v0x58c9a24748c0_0, v0x58c9a2466c70_0, v0x58c9a243eba0_0 {0 0 0};
T_0.1 ;
    %end;
